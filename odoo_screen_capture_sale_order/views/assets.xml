<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_frontend_js_override" inherit_id="website.assets_frontend" priority="1">
        <!-- Customize javascript -->
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/odoo_screen_capture_sale_order/static/src/js/screen_capture_sale_order.js"/>
        </xpath>
    </template>

    <template id="_assets_frontend_scss_override" inherit_id="website.assets_frontend" priority="2">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/odoo_screen_capture_sale_order/static/src/scss/bootstrap-override.scss"/>
        </xpath>
    </template>
</odoo>